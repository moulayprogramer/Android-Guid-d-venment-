<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phoenix.moulay.guidevenment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
        android:name=".Splash"
        android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Map"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Map" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Conferencier"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Conferencier" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Conferences"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Conferences" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Places"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Places" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Info"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Info" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".Pregramme"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.phoenix.moulay.guidevenment.Pregramme" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>